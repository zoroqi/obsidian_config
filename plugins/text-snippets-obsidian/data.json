{
  "snippets_file": "w : [word::$end$] ``\nph : [pharse::$end$]\nred : [related::$end$]\narc : [archive]($end$)\nnote : > [!NOTE]",
  "snippets": [
    "w : [word::$end$] ``",
    "ph : [pharse::$end$]",
    "red : [related::$end$]",
    "arc : [archive]($end$)",
    "note : > [!NOTE]"
  ],
  "endSymbol": "$end$",
  "newlineSymbol": "$nl$",
  "stopSymbol": "$tb$",
  "pasteSymbol": "$pst$",
  "useTab": false,
  "useSpace": false,
  "wordDelimiters": "$()[]{}<>,.!?;:'\"\\/",
  "isWYSIWYG": true
}